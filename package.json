{
  "name": "my-project",
  "packageManager": "yarn@3.5.1",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "bugs": {},
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "server",
    "test": "test"
  },
  "main": "server/index",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "node server/index.js",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "npm run mocha"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.1",
    "@feathersjs/authentication": "^5.0.1",
    "@feathersjs/authentication-client": "^5.0.2",
    "@feathersjs/authentication-local": "^5.0.5",
    "@feathersjs/authentication-oauth": "^5.0.5",
    "@feathersjs/configuration": "^5.0.1",
    "@feathersjs/errors": "^5.0.1",
    "@feathersjs/feathers": "^5.0.1",
    "@feathersjs/koa": "^5.0.1",
    "@feathersjs/mongodb": "^5.0.1",
    "@feathersjs/schema": "^5.0.1",
    "@feathersjs/socketio": "^5.0.1",
    "@feathersjs/transport-commons": "^5.0.1",
    "@feathersjs/typebox": "^5.0.1",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "koa-send": "^5.0.1",
    "koa-views": "^8.0.0",
    "mithril": "^2.2.2",
    "mithril-node-render": "^3.0.2",
    "mongodb": "^5.1.0",
    "winston": "latest"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.2",
    "@feathersjs/rest-client": "^5.0.1",
    "axios": "^1.3.4",
    "cross-env": "latest",
    "mocha": "^10.2.0",
    "nodemon": "latest",
    "prettier": "latest",
    "vite": "^2.6.2"
  }
}
